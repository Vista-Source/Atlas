
#include "Atlas.h"

#ifdef __cplusplus
extern "C" {
#endif

{{ for method in methods }}
ATLAS_EXPORT {{ method.return_type }} ATLAS_{{ method.name }}({{ method.parameters }})
{
{{- if method.return_type != "void" }}
    return {{ method.body }}
{{ else }}
    {{ method.body }}
{{ end }}
}
{{ end }}

#ifdef __cplusplus
}
#endif
